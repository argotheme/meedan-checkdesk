  $translations['ar:default']['4e1a63bcfba4f03ef8e777af192c4955'] = array(
    'source' => 'Users can create a poll by clicking on Poll on the <a href="@add-content">Add new content</a> page, and entering the question being posed, the answer choices, and beginning vote counts for each choice. The status (closed or active) and duration (length of time the poll remains active for new votes) can also be specified.',
    'context' => '',
    'location' => '',
    'translation' => 'بإمكان المستخدمين إنشاء استبيان بالنقر فوق الاستبيان في صفحة <a href="@add-content">إضافة محتوى جديد</a>، ثم كتابة سؤال، والأجوبة المحتملة وتعيين القيمة المبدئية لعدد اﻷصوات. كما يمكن أيضاً تحديد الحالة (مغلق أو مفتوح) والمدة (الفترة التي يبقى فيها الاستطلاع مفتوح للتصويت).',
    'plid' => 0,
    'plural' => 0,
  );
  $translations['ar:default']['500a7b7ad5d38affd8c21a8418a6754d'] = array(
