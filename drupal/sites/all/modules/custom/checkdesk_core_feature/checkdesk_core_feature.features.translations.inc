  $translations['ar:default']['dd5f9da1fa9a38058d933e013833e922'] = array(
    'source' => 'The Color module allows users with the <em>Administer site configuration</em> permission to quickly and easily change the color scheme of themes that have been built to be compatible with it. For more information, see the online handbook entry for <a href="@color">Color module</a>.',
    'context' => '',
