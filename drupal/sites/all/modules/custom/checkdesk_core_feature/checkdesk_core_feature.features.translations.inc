  $translations['ar:default']['2ce8830341a6dbfb5fa175b5794d59a1'] = array(
    'source' => 'This page displays a list of the existing custom profile fields to be displayed on a user\'s <em>My account</em> page. To provide structure, similar or related fields may be placed inside a category. To add a new category (or edit an existing one), edit a profile field and provide a new category name.',
