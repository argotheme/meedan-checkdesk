  $translations['ar:default']['84485d2a3f53176fa6d9f7f40ae8202d'] = array(
    'source' => 'Thousands of sites (particularly news sites and blogs) publish their latest headlines and posts in feeds, using a number of standardized XML-based formats. Formats supported by the aggregator include <a href="@rss">RSS</a>, <a href="@rdf">RDF</a>, and <a href="@atom">Atom</a>.',
